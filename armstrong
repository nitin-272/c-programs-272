#include<stdio.h>
int main()
{
	
	int num,rem,cube,sum=0,temp=0;
	printf("enter the number ");
	scanf("%d",&num);
	temp=num;
	for(;num!=0;)
	{
		rem=num%10;
		cube=rem*rem*rem;
		sum=sum+cube;
		num=num/10;
	}
	if(sum==temp)
	printf(" armstrong number ");
	else
	printf(" not a armstroing number ");
}